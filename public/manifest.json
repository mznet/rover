{
  "name": "rover",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Search your bookmarks easily.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "bookmarks"
  ],
  "commands": {
    "run-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+O",
        "mac": "Command+Shift+O"
      },
      "description": "Run \"foo\" on the current page."
    }
  },
  "background": {
    "service_worker": "./static/js/background.js",
    "type": "module"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}